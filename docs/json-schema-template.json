          {
          "name": "property-name",
          "value": "property-value"
          }
      ],
      "steps": [
        {
          "name": "step-name",
          "description": "step-description",
          "type": "navigate, click, input, select, wait, assert, execute",
          "selector-type": "css, xpath, id, name, class, tag",
          "selector": "selector-value",
          "value": "value",
          "mandatory": true
        }
      ]
    }
  ]
}